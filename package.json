{
  "name": "teams-snap",
  "version": "1.0.0",
  "description": "A snap of Teams",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --linux snap"
  },
  "build": {
    "productName": "Teams-Snap",
    "linux": {
      "target": "snap",
      "category": "Productivity,Utility",
      "icon": "icon.png"
    }
  },
  "keywords": [
    "microsoft",
    "teams",
    "snapcraft"
  ],
  "author": "Cong Vo",
  "license": "MIT",
  "devDependencies": {
    "electron": "29.2.0",
    "electron-builder": "24.13.3",
    "electron-installer-snap": "5.2.0"
  },
  "dependencies": {
    "pnpm": "9.2.0"
  }
}
